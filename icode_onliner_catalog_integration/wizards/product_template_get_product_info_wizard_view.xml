<odoo>
    <record model="ir.ui.view" id="product_template_get_product_info_wizard_view">
        <field name="name">product.template.get_product_info_wizard.form.view</field>
        <field name="model">product.template.get_product_info_wizard</field>
        <field name="arch" type="xml">
            <form string="Get Product Info">
                <sheet>
                    <group>
                        <field name="product_id"/>
                        <field name="product_category"/>
                        <field name="vendor"/>
                        <field name="model"/>
                        <field name="price"/>
                        <field name="currency"/>
                        <field name="comment"/>
                        <field name="producer"/>
                        <field name="importer"/>
                        <field name="warranty"/>
                        <field name="deliveryTownTime"/>
                        <field name="deliveryTownPrice"/>
                        <field name="deliveryCountryTime"/>
                        <field name="deliveryCountryPrice"/>
                        <field name="productLifeTime"/>
                        <field name="isCashless"/>
                        <field name="isCredit"/>
                        <field name="stockStatus"/>
                        <field name="courierDeliveryPrices"/>
                    </group>
                </sheet>
                <footer>
                    <button string="Apply" class="btn-primary" type="object" name="publicize_on_onliner"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>